{% extends 'BORAdminBundle::layout.html.twig' %}

{% block content %}
    <h1>QcmPoint list</h1>
    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Qcmpoints</th>
                <th>Experience</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('bor_qcmpoint_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.qcmPoints }}</td>
                <td>{{ entity.experience }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('bor_qcmpoint_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('bor_qcmpoint_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    {% endblock %}
