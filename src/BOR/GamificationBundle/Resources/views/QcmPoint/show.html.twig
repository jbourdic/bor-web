{% extends 'BORAdminBundle::layout.html.twig' %}

{% block content %}
    <h1>QcmPoint</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Qcmpoints</th>
                <td>{{ entity.qcmPoints }}</td>
            </tr>
            <tr>
                <th>Experience</th>
                <td>{{ entity.experience }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('bor_qcmpoint_index') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('bor_qcmpoint_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
</ul>
{% endblock %}
