{% extends 'BORAdminBundle::layout.html.twig' %}

{% block content -%}
    <h1>BlockAdvert</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Reason</th>
                <td>{{ entity.reason }}</td>
            </tr>
            <tr>
                <th>Idadvert</th>
                <td>{{ entity.idAdvert }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('bor_blockadvert_index') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('bor_blockadvert_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
